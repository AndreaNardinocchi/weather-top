{{#each stations}}
<div class="box box-link-hover-shadow">
  <h2 class="title">
    {{title}} 
  </h2>
  <h3 class="subtitle pt-4">
   Lat: {{latitude}} <br>
   Long: {{longitude}}
  </h3>
  <a href="/station/{{_id}}" class="button">
    {{>icons/open}}
  </a>
  <a href="/dashboard/deletestation/{{_id}}" class="button">

    {{>icons/delete}}

  </a>
</div>
{{/each}}